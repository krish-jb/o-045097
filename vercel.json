 {
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api).*)",
            "has": [
                {
                    "type": "header",
                    "key": "user-agent",
                    "value": ".*(WhatsApp|whatsapp|facebookexternalhit|twitterbot|discord|linkedin|telegram).*"
                }
            ],
            "destination": "/api/og-meta?username=$1"
        },
        {
            "source": "/((?!api).*)",
            "destination": "/index.html"
        }
    ]
}
